{
    "source": {
      "include": ["index.js"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "docs/web/",
      "template": "docs/page-template",
      "readme": "docs/page-template/README.md"
    }
  }